{
  "name": "react-webcomponents",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-to-webcomponent": "^1.5.1",
    "recharts": "^2.0.9",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "rescripts start",
    "debug": "serve dist",
    "build": "yarn run build:css && yarn run build:standalone",
    "build:externals": "NODE_ENV=production rollup -c",
    "build:standalone": "NODE_ENV=production rollup -c rollup.standalone.js",
    "build:example": "rescripts build",
    "test": "rescripts test",
    "test-coverage": "rescripts test --coverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.14.5",
    "@princed/rescript-disable-eslint": "^2.0.0",
    "@rescripts/cli": "^0.0.16",
    "@rescripts/rescript-use-babel-config": "^0.0.12",
    "@rescripts/utilities": "^0.0.8",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-backgrounds": "^5.3.21",
    "@storybook/addon-centered": "^5.3.21",
    "@storybook/addon-docs": "^5.3.21",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addons": "^5.3.21",
    "@storybook/cli": "^5.3.21",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "babel-jest": "26.6.0",
    "babel-loader": "8.1.0",
    "babel-plugin-rewire": "^1.2.0",
    "babel-preset-react-app": "^10.0.0",
    "core-js": "^3.6.5",
    "dotenv-webpack": "^1.7.0",
    "eslint-config-benjamin-van-ryseghem": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-i18n-translated": "^1.0.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-utils": "^2.0.0",
    "http-proxy-middleware": "^1.0.3",
    "jest-enzyme": "^7.1.2",
    "prop-types": "^15.7.2",
    "rescript-disable-eslint": "^1.0.1",
    "serve": "^12.0.0",
    "storybook": "^6.2.9"
  }
}
